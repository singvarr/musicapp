{
    "useTabs": false,
    "printWidth": 80,
    "tabWidth": 4,
    "singleQuote": false,
    "bracketSpacing": true,
    "trailingComma": "es5",
    "jsxBracketSameLine": false,
    "parser": "babylon",
    "semi": true,
    "overrides": [
        {
            "files": ["*.less", "*.css"],
            "options": {
                "parser": "less"
            }
        }
    ]
}
